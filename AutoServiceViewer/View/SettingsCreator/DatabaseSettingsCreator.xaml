<UserControl x:Class="AutoServiceViewer.View.SettingsCreator.DatabaseSettingsCreator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutoServiceViewer.View.SettingsCreator"
             mc:Ignorable="d" 
             xmlns:viewmodels="clr-namespace:AutoServiceViewer.ViewModel"
             d:DataContext="{d:DesignData viewmodels: DatabaseSettingsViewModel}"
             d:DesignHeight="105" d:DesignWidth="105">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Width="105">
            <Button Command="{Binding OpenFileCommand}" Width="100" Height="20" Content="Выбрать файл"/>
            <Label> Connection String</Label>
            <TextBox Width="Auto" Height="30"></TextBox>

        </StackPanel>

    </Grid>
</UserControl>
